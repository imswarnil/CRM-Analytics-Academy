---
layout: null
---
[
  {%- assign all_docs = site.documents | where_exp: "doc", "doc.title" -%}
  {%- for doc in all_docs -%}
    {
      "title": {{ doc.title | jsonify }},
      "content": {{ doc.content | strip_html | strip_newlines | jsonify }},
      "url": "{{ doc.url | relative_url }}",
      "category": "{{ doc.collection | default: 'Page' | capitalize }}",
      "icon": "{{ doc.icon | default: 'ph-file-text' }}"
    }
    {%- unless forloop.last -%},{%- endunless -%}
  {%- endfor -%}
]